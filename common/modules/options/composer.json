{
  "name": "backend/options",
  "description": "Storage application options (settings) in the database",
  "keywords": ["yii2", "yii2-options", "settings", "options", "itlo"],
  "type": "yii2-extension",
  "license": "MIT",
  "version": "1.6.0",
  "homepage": "https://github.com/itlo-cms/options",
  "support": {
    "source": "https://github.com/itlo-cms/options",
    "issues": "https://github.com/itlo-cms/options/issues"
  },
  "authors": [
    {
      "name": "Roman Logachev",
      "email": "rlogachev@itlo.ru"
    },
    {
      "name": "Infomarket Group, Russia",
      "email": "info@infomarketstudio.ru"
    }
  ],
  "require": {
    "yiisoft/yii2": "^2.0.35",
    "wdmg/yii2-base": "^1.3.0",
    "wdmg/yii2-selectinput": "^1.0.9",
    "wdmg/yii2-helpers": "^1.4.1"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "wdmg\\options\\": ""
    }
  },
  "extra": {
    "options": {
      "routePrefix": "admin",
      "autoloadOptions": true
    }
  }
}